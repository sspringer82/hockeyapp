<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Document</title>
  </head>
  <body>
    <h1>Standings</h1>
    <table>
      <thead>
        <tr>
          <td></td>
          <td>Team</td>
          <td>GP</td>
          <td>PTS</td>
          <td>DIFF</td>
        </tr>
      </thead>
      <tbody>
        <% standings.map((standing, index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td>
            <img height="18" src="/<%= standing.logo %>" /><%= standing.name %>
          </td>
          <td><%= standing.gp %></td>
          <td><%= standing.pts %></td>
          <td><%= standing.diffP + ' - ' + standing.diffM %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
